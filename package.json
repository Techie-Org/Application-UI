{
  "name": "artistry",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "webpack:build": "webpack",
    "start:server": "node ./src/server",
    "test": "npm run test:default -- --coverage",
    "test:fast": "npm run test:default",
    "test:default": "node --expose-gc ./node_modules/jest-cli/bin/jest.js",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js",
    "lint:css": "stylelint \"src/**/*.scss\" \"src/**/*.css\" --cache",
    "lint:eslint": "eslint --quiet --cache --ignore-path .gitignore --ignore-pattern \"internals/scripts\" --ignore-pattern static --ignore-pattern \"static/*\"",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "verify-branch-naming": "node ./internals/scripts/verify-branch-naming",
    "prepare": "husky",
    "extract-intl": "extract-messages -l=en -o ./src/translations -d en --flat true ./src/**/messages.js"
  },
  "lint-staged": {
    "*.js": "npm run lint:eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@react-google-maps/api": "^2.19.3",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "history": "^5.3.0",
    "http-proxy-middleware": "^3.0.1",
    "immutable": "^4.3.7",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.6.8",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.26.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^5.0.1",
    "react-swipeable": "^7.0.1",
    "redux": "^4.2.1",
    "redux-form": "^8.3.10",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.3.0",
    "reselect": "^5.1.1",
    "tunnel": "^0.0.6",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "babel-plugin-module-resolver": "^5.0.2",
    "css-loader": "^7.1.2",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-redux-saga": "^1.3.2",
    "express": "^4.20.0",
    "extract-react-intl-messages": "^4.1.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "shelljs": "^0.8.5",
    "stylelint": "^16.9.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.5.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-hot-middleware": "^2.26.1"
  }
}
