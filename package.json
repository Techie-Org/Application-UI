{
  "name": "artistry",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --quiet --cache --ignore-path .gitignore --ignore-pattern \"internals/scripts\" \"internals/generators\" --ignore-pattern static --ignore-pattern \"static/*\"",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "stylelint \"app/**/*.scss\" \"app/**/*.css\" --cache",
    "verify-branch-naming": "node ./internals/scripts/verify-branch-naming",
    "prepare": "husky install",
    "extract-intl": "extract-messages -l=en -o ./src/translations -d en --flat true ./src/**/messages.js"
  },
  "lint:staged": {
    "*.js": "npm run lint:eslint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@react-google-maps/api": "^2.19.3",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "history": "^5.3.0",
    "http-proxy-middleware": "^3.0.1",
    "immutable": "^4.3.7",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^6.6.8",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.26.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^5.0.1",
    "react-swipeable": "^7.0.1",
    "redux": "^4.2.1",
    "redux-form": "^8.3.10",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.3.0",
    "reselect": "^5.1.1",
    "tunnel": "^0.0.6",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.57.0",
    "extract-react-intl-messages": "^4.1.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "sass": "^1.77.8",
    "shelljs": "^0.8.5",
    "stylelint": "^16.9.0",
    "stylelint-scss": "^6.5.1"
  }
}
